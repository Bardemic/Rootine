{
  "name": "rootine-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "npm run dev --workspace=backend",
    "dev:frontend": "npm run dev --workspace=frontend",
    "build": "npm run build --workspace=backend && npm run build --workspace=@rootine/api-types && npm run build --workspace=frontend"
  },
  "devDependencies": {
    "@types/tough-cookie": "^4.0.5",
    "concurrently": "^8.2.2"
  }
}

